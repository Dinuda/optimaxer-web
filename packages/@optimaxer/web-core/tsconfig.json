    "outDir": "dist",
    "rootDir": "src",
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "types": ["node","@mediapipe/tasks-genai"],
    "skipLibCheck": true,
  },
  "include": ["src/**/*"]
}